<nav role="navigation" aria-label="{{ 'skiplinks.label' | translate }}">
  <ul class="navbar-nav">
    {%- for link in navigation.top[locale] -%}
      <li class="nav-item{{ " hidden-if-no-js" if link.hiddenIfNoJS === true }}">
        <a href="{{ link.href }}" class="nav-link">{{ link.label }}</a>
      </li>
    {%- endfor -%}
  </ul>
</nav>
